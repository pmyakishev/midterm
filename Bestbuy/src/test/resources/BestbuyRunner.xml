<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="Tsunami on Chrome Firefox">

    <parameter name="useCloudEnv" value="true"/>
    <parameter name="cloudEnv" value="Browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="resolution" value="1024x768"/>
    <parameter name="url" value="http://www.bestbuy.com"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="55"/>
    <parameter name="testName" value="TestBestBuyApp"/>

    <test name = "Page">
        <classes>
            <class name="Page.TestUserLogin"/>
            <!--<class name="Page.TestSearchItems"/>-->
            <class name="Page.TestHomePage"/>
        </classes>
    </test>
</suite>